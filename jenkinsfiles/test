pipeline {
  agent any
  options {
    skipDefaultCheckout(true)
  }
  stages {
    stage('Prepare') {
      steps {
        cleanWs()
        checkout scm
        sh 'make init'
        sh 'make depatch'
        sh 'make patch'
      }
    }
    stage('Generate emulator') {
      steps {
        sh 'mill tests.emulator.elf'
      }
    }
    stage('tests') {
      steps {
        sh 'make test'
      }
    }
  }
}
