pipeline {
  agent any
  options {
    skipDefaultCheckout(true)
  }
  stages {
    stage('Prepare') {
      steps {
        cleanWs()
        checkout scm
        sh 'make init'
        sh 'make depatch'
        sh 'make patch'
      }
    }
    stage('Generate emulator') {
      steps {
        sh 'mill tests.emulator.elf'
      }
    }
    stage('rv64mi-p') {
      steps {
        sh 'mill tests.run.rv64default.rv64mi-p.report'
      }
    }
    stage('rv64mi-p-ld') {
      steps {
        sh 'mill tests.run.rv64default.rv64mi-p-ld.report'
      }
    }
    stage('rv64mi-p-lh') {
      steps {
        sh 'mill tests.run.rv64default.rv64mi-p-lh.report'
      }
    }
    stage('rv64mi-p-lw') {
      steps {
        sh 'mill tests.run.rv64default.rv64mi-p-lw.report'
      }
    }
    stage('rv64mi-p-sd') {
      steps {
        sh 'mill tests.run.rv64default.rv64mi-p-sd.report'
      }
    }
    stage('rv64mi-p-sh') {
      steps {
        sh 'mill tests.run.rv64default.rv64mi-p-sh.report'
      }
    }
    stage('rv64mi-p-sw') {
      steps {
        sh 'mill tests.run.rv64default.rv64mi-p-sw.report'
      }
    }
    stage('rv64si-p') {
      steps {
        sh 'mill tests.run.rv64default.rv64si-p.report'
      }
    }
    stage('rv64si-cache') {
      steps {
        sh 'mill tests.run.rv64default.rv64si-p-icache.report'
      }
    }
    stage('rv64ua-p') {
      steps {
        sh 'mill tests.run.rv64default.rv64ua-p.report'
      }
    }
    stage('rv64ua-v') {
      steps {
        sh 'mill tests.run.rv64default.rv64ua-v.report'
      }
    }
    stage('rv64uc-p') {
      steps {
        sh 'mill tests.run.rv64default.rv64uc-p.report'
      }
    }
    stage('rv64uc-v') {
      steps {
        sh 'mill tests.run.rv64default.rv64uc-v.report'
      }
    }
    stage('rv64ud-p') {
      steps {
        sh 'mill tests.run.rv64default.rv64ud-p.report'
      }
    }
    stage('rv64ud-v') {
      steps {
        sh 'mill tests.run.rv64default.rv64ud-v.report'
      }
    }
    stage('rv64uf-p') {
      steps {
        sh 'mill tests.run.rv64default.rv64uf-p.report'
      }
    }
    stage('rv64uf-v') {
      steps {
        sh 'mill tests.run.rv64default.rv64uf-v.report'
      }
    }
    stage('rv64um-p') {
      steps {
        sh 'mill tests.run.rv64default.rv64um-p.report'
      }
    }
    stage('rv64um-v') {
      steps {
        sh 'mill tests.run.rv64default.rv64um-v.report'
      }
    }
  }
}
